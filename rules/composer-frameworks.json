{
    "rules": [
        {
            "name": "php-composer",
            "args": ["composer.json", "app/composer.json"],
            "strg": "file-exist"
        },
        {
            "name": "php-framework-symfony",
            "args": ["symfony/symfony"],
            "strg": "composer#d"
        },
        {
            "name": "php-framework-laravel",
            "args": ["laravel/framework"],
            "strg": "composer#d"
        },
        {
            "name": "php-framework-cakephp",
            "args": ["cakephp/cakephjp"],
            "strg": "composer#d"
        },
        {
            "name": "php-framework-slim",
            "args": ["slim/slim"],
            "strg": "composer#d"
        },
        {
            "name": "php-framework-workerman",
            "args": ["workerman/workerman"],
            "strg": "composer#d"
        },
        {
            "name": "php-framework-october",
            "args": ["october/system"],
            "strg": "composer#d"
        },
        {
            "name": "php-framework-october",
            "args": ["name", "october/october"],
            "strg": "composer#f"
        },
        {
            "name": "php-framework-yii2",
            "args": ["yiisoft/yii2"],
            "strg": "composer#d"
        }
    ]
   
}